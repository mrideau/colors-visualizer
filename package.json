{
  "name": "colors-show-runner",
  "version": "0.0.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "dev": "npm-run-all ng:serve && electron:serve",
    "ng:serve": "ng serve",
    "build": "ng build --base-href ./",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:electron": "ng build --base-href ./ && electron .",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron app.js --serve",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.2.9",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@electron/remote": "^2.0.9",
    "@ng-bootstrap/ng-bootstrap": "^14.2.0",
    "@ngx-formly/bootstrap": "^6.1.7",
    "@ngx-formly/core": "^6.1.7",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "electron-debug": "^3.2.0",
    "electron-reloader": "^1.2.3",
    "electron-settings": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "rxjs": "~7.5.0",
    "wait-on": "^7.0.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.2",
    "@angular/cli": "~15.0.2",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/localize": "^15.0.0",
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@ngx-formly/schematics": "^6.1.7",
    "@tauri-apps/cli": "^1.3.1",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.2.5",
    "electron": "^24.4.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "typescript": "~4.8.2"
  }
}
